.box,page{
  width: 100%;
  height: 100%;
}
.head{
    border-bottom: 2rpx solid #eee;
    height: 13%;
}
.headbox{
  width: 90%;
  margin: 0 auto;
    height: 100%;
}
.headdate{
  display: flex;
  
  height: 100%;
}
.headdate view{
  flex: 1
}
.date,.changedate{
  box-sizing: border-box;
  padding-top: 40rpx;
    height: 100%;
}
.changedate{
  text-align: center;
}
.date{
  border-right: 2rpx solid #eee;
    
}
.showbox{
  height: 8%;
  display: flex;
  border-bottom: 2rpx solid #eee;
  overflow-x:scroll;
}
.pro,.shownum{
  padding: 0 20rpx;
  margin: 25rpx 20rpx;
  background: #eee;
  border-radius: 40rpx;
  font-size: 28rpx;
  line-height: 52rpx; 
}

.shownum{
  white-space:nowrap;
}
.content{
  display: flex;
  height: 78%;
    overflow: hidden;
}
.left{
  width: 30%;
  background: #eee;
overflow: auto;
}
.nav{
  height: 102rpx;
  line-height: 102rpx;
  text-align: center;
}
.color{
  border-left: #9b72e8 solid 2rpx;
  background: #fff;
}
.color1{
  background: #9b72e8
}
.color2{
  background: #eee
}
button::after{

    border:none;

}
.pro{
  margin-bottom: 0;
}
.right{
  width: 70%;

}
button{
  display: inline-block;
}
.confirm{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  background: rgba(31,31,31,0.68)
  }