page{
  width: 100%;
  height: 100%;
  background: #eee;
}
.header{
  width: 100%;
  height: 88rpx;
      background: rgb(110, 51,208);
    border-bottom: 2rpx solid #fff;
    display: flex;
}
.header>view{
  min-width: 96rpx;
  position: relative;
  border-right: 2rpx solid #fff;
}
.cart,.user,.back,.classify{
  margin:  auto;
  position: absolute;
     width: 40rpx;
    height: 36rpx;
    top: 26rpx;
    left: 30rpx;
}
.back{
   background: url("data:image/png;base64,") no-repeat;
    background-size: contain;
}
.classify{
  background: url("http://wt.lingdie.com/tpl/static/store/caticon3.png") no-repeat;
    background-size: contain;

}

.tit{
  color: #fff;
  font-size:36rpx;
      line-height: 88rpx;
      font-weight: 700;
    text-align: center;
    width: 356rpx;
}
.user{
  background: url("http://wt.lingdie.com/tpl/static/store/user3.png") no-repeat;
  background-size: contain;
}
.cart{
  background: url("http://wt.lingdie.com/tpl/static/store/cart3.png") no-repeat;
  background-size: contain;
}

.m-cart-e{
  width: 670rpx;
  height: 416rpx;
  padding: 40rpx;
  background: #fff;
}

.m-cart-e .icon{
  width: 270rpx;
  height: 200rpx;
  margin: 20rpx auto 20rpx;
  background: url("http://wt.lingdie.com/tpl/Wap/default/common/css/store/css/img/cart-e.png")no-repeat;
  background-size: contain;
    padding-right: 40rpx;
}
.txt{
      font-size: 40rpx;
    font-weight: 700;
    color: #ccc;
    text-align: center;
}
.gobuy{
      line-height: 84rpx;
    display: block;
    border-radius: 4rpx;
    background: rgb(110, 51,208);
    color: #fff;
    font-size: 32rpx;
    text-shadow: none;
    margin: 30rpx;
    font-weight: 700;
    text-align: center;
}
.menu{
   position: absolute;
    margin: 16rpx 0 0 8rpx;
    background: rgba(0,0,0,0.8);
    width: 376rpx;
    z-index: 99;
    box-shadow: 0 0 6rpx rgba(0, 0, 0, 0.8);

}
.ico{
  position: absolute;
    top: -16rpx;
    left: 124rpx;
    content: '';
    width: 0;
    height: 0;
    border: 16rpx solid transparent;
    border-bottom: 16rpx solid rgba(0, 0, 0, 0.8);
    border-top: 0;
}
.num{
  display: flex;
  flex-wrap: wrap;
}
.num view{
      width: 94px;
    line-height: 40px;
    text-align: center;
    border-right: 1px solid #181818;
    border-bottom: 1px solid #181818;
    margin-right: -1px;
    font-size: 16px;
    color: #fff;
}